<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.MessageDao">

    <insert id="createMessage" parameterType="entity.User">
      INSERT INTO message(sender, sender_kind, receiver, receiver_kind, content)
      VALUE (#{sender.})
    </insert>

</mapper>